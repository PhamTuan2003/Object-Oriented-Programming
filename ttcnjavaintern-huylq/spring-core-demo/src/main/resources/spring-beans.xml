<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <import resource="conf/accountService.xml"/>

    <bean id="beanPrinterProcessor" class="vn.com.vds.config.BeanPrinterProcessor" />

    <bean id="configurableEnvironment" class="org.springframework.core.env.StandardEnvironment" />

    <bean id="appConfig" class="vn.com.vds.config.AppConfig">
        <constructor-arg name="configurableEnvironment" ref="configurableEnvironment" />
    </bean>

</beans>